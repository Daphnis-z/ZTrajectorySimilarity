<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	 <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="false" />
    <package name="default" namespace="/" extends="struts-default">
       <default-action-ref name="index"></default-action-ref>
        <action name="index">  
            <result>jsp/default.jsp</result>  
        </action> 
        <action name="doublePattern">  
            <result>jsp/doubleTraj/doubleTraj.jsp</result>  
        </action> 
        <action name="morePattern">  
            <result>jsp/moreTraj/moreTraj.jsp</result>  
        </action> 
    	<action name="doubleTrajCul" class="com.adx.action.DoubleTrajAction">
    		<result name="success">doubleTrajResult.jsp</result>
    		<result name="input">doubleTrajResult.jsp</result>
    		<result name="error">doubleTrajResult.jsp</result>
    		<result name="none">doubleTrajResult.jsp</result>
    	</action>
    	<action name="moreTrajCul" class="com.adx.action.MoreTrajAction">
    		<result name="success">moreTrajResult.jsp</result>
    		<result name="input">moreTrajResult.jsp</result>
    		<result name="error">moreTrajResult.jsp</result>
    		<result name="none">moreTrajResult.jsp</result>
    	</action>
    	
    	<!-- 在GIS系统中显示轨迹 -->
    	<action name="viewTraj" class="com.daphnis.gis.TrajPolyline">
    		<result name="success">viewTraj.jsp</result>
    		<result name="none">viewTraj.jsp</result>
    	</action>     
    </package>
</struts>

