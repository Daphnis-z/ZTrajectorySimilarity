<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	 <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="false" />
    <package name="default" namespace="/" extends="struts-default">
       <default-action-ref name="index"></default-action-ref>
        <action name="index">  
            <result>jsp/default.jsp</result>  
        </action> 
    	<action name="doubleTrajCul" class="com.adx.action.DoubleTrajAction">
    		<result name="success">jsp/doubleTrajResult.jsp</result>
    	 	<result name="error">jsp/inputError.jsp</result>  		
    	</action>
    	
    	<!-- 在GIS系统中显示轨迹 -->
    	<action name="showPolyline" class="com.daphnis.gis.TrajPolyline">
    		<result>jsp/gis/showPolylineTraj.jsp</result>
    	</action>   
 </package>
</struts>